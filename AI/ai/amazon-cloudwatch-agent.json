{
    "agent": {
        "metrics_collection_interval": 3600
    },
    "metrics": {
        "append_dimensions": {
            "InstanceId": "${aws:InstanceId}"
        },
        "metrics_collected": {
            "disk": {
                "measurement": ["used_percent"],
                "metrics_collection_interval": 3600,
                "resources": ["/"]
            }
        }
    }
}
